///solution 1//////////////

<html>
<head></head>
<body>

<script>
var data=[
  {
    id:1,
    type:"Bmw"
  },
  {
    id:2,
    type:"audi"
  },
  {
    id:3,
    type:"Bmw"
  },
  {
    id:4,
    type:"mercedes"
  },
  {
    id:5,
    type:"ferrari"
  },{
    id:6,
    type:"audi"
  },
  {
    id:7,
    type:"audi"
  },
  {
    id:8,
    type:"ferrari"
  }
]





var finalResult=[];

for(var i=0;i<data.length;i++)
{
   var response={};
    var id_array=[];
	var objName=data[i].type;
    response['type']=objName;
     var k=0;
    for(var j=0;j<data.length;j++)
     {
          var found = finalResult.some(function (el) {
                  return el.type === objName;
         });
      
          if(found) {
            k++;
            break;
               
           }
          if(data[j].type==objName)
          {
              id_array.push(data[j].id);
          }
     }
     if(k===0){
          response['ids']=id_array;
          finalResult.push(response);       
      }
}

document.write(JSON.stringify(finalResult));

</script>
</body>
</html>


//////////solution 2/////////////////

<html>
<head></head>
<body>

<script>
 var data=[
  {
    id:1,
    type:"Bmw"
  },
  {
    id:2,
    type:"  audi  "
  },
  {
    id:3,
    type:"Bmw"
  },
  {
    id:4,
    type:"mErcedes"
  },
  {
    id:5,
    type:"feRrari"
  },{
    id:6,
    type:"audi"
  },
  {
    id:'7',
    type:"audi"
  },
  {
    id:8,
    type:"ferrari"
  }
];
var finalRes = [];
for(var i=0; i<data.length; i++){
	var existObj = finalRes.find((item) => item['type'] === data[i].type.trim().toLowerCase());
    var id=Number(data[i].id);
	if(existObj){
		existObj.ids.push(id);
	} else {
 		finalRes.push({
			type: data[i].type.trim().toLowerCase(),
			ids :[id]
		})
	}
}
console.log(finalRes);

</script>
</body>
</html>





































































