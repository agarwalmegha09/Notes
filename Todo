        var i=0;
        tot_marks_termwise[i]=0;
        var finalMarksObject ={};
        studentList.forEach((studentData) => {

            tot_marks_termwise[i]=0;
            finalMarksObject['term_no_' + studentData['term_no']] = {};

            studentData.subject.forEach((sub) => {
                var objName = sub.subject_name;
                var objValue = sub.marks_obtained;
                tot_marks_termwise[i] = tot_marks_termwise[i] + objValue;
                finalMarksObject['term_no_' + studentData['term_no']][objName] = objValue;
            });
            finalMarksObject['term_no_' + studentData['term_no']]['Total_marks_for_term'+ studentData['term_no']] = tot_marks_termwise[i];
            i++;


        });
        res.end(JSON.stringify([finalMarksObject,tot_marks_termwise]));









